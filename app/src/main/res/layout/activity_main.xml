<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">




    <TextView
        android:id="@+id/viewPrice"
        android:layout_width="0dp"
        android:layout_height="105dp"
        android:layout_marginTop="8dp"
        android:text="0"
        android:textAlignment="textStart"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="80sp"
        app:layout_constraintWidth_percent="0.4"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <Button
        android:id="@+id/buttonPrintReciept"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="261dp"
        android:layout_height="108dp"
        android:background="@color/colorPrimary"
        android:text="In hoa don"
        android:textColor="@android:color/white"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline15"
        app:layout_constraintEnd_toStartOf="@+id/guideline14" />





    <TextView
        android:id="@+id/viewCalcOperation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/viewPrice"
        app:layout_constraintStart_toStartOf="@+id/guideline6" />

    <TextView
        android:id="@+id/addEmpNameText"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/viewPrice"
        app:layout_constraintStart_toEndOf="@+id/viewPrice"
        app:layout_constraintWidth_percent="0.6" />

    <ListView
        android:id="@+id/transactionListView"
        android:layout_width="0dp"
        android:layout_height="416dp"
        app:layout_constraintEnd_toStartOf="@+id/leftNumPad"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/viewPrice"
        app:layout_constraintTop_toTopOf="@+id/guideline18"
        app:layout_constraintWidth_percent="1"></ListView>

    <TextView
        android:id="@+id/transSumText"
        android:layout_width="0dp"
        android:layout_height="72dp"
        android:layout_marginTop="50dp"
        android:text="dang tính toán"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="@+id/transactionListView"
        app:layout_constraintStart_toStartOf="@+id/transactionListView"
        app:layout_constraintTop_toBottomOf="@+id/transactionListView"
        app:layout_constraintWidth_percent="1"
        />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tong Ngay"
        app:layout_constraintBottom_toTopOf="@+id/transSumText"
        app:layout_constraintStart_toStartOf="@+id/transSumText" />

    <Button
        android:id="@+id/finishMainActivityButton"
        android:layout_width="146dp"
        android:layout_height="45dp"
        android:text="Kết thúc"
        app:layout_constraintEnd_toStartOf="@+id/guideline14"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <Button
        android:id="@+id/printEetButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:text="In EET"
        app:layout_constraintBottom_toTopOf="@+id/guideline15"
        app:layout_constraintEnd_toStartOf="@+id/buttonPrintReciept" />

    <Button
        android:id="@+id/employeeEetReceiptButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="Show EET"
        app:layout_constraintBottom_toBottomOf="@+id/printEetButton"
        app:layout_constraintEnd_toStartOf="@+id/printEetButton" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/leftNumPad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.3
" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="1"
        app:layout_constraintWidth_percent="1"

        app:layout_constraintBottom_toTopOf="@+id/guideline17"
        app:layout_constraintEnd_toStartOf="@+id/guideline14"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/leftNumPad"
        app:layout_constraintTop_toTopOf="@+id/guideline18">

        <Button
            android:id="@+id/buttonErase"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="0px"
            android:layout_height="0dp"
            android:background="@color/colorAccent"
            android:text="Xoa"
            android:textColor="@android:color/white"
            android:textSize="30sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noSeven"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0dp"
            android:text="7"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toEndOf="@+id/buttonErase"
            app:layout_constraintTop_toTopOf="@+id/buttonErase"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noEight"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="8"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toEndOf="@+id/noSeven"
            app:layout_constraintTop_toTopOf="@+id/noSeven"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noNine"
            android:layout_width="0dp"
            android:layout_height="0sp"
            android:text="9"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toEndOf="@+id/noEight"
            app:layout_constraintTop_toTopOf="@+id/noEight"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noFive"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0dp"
            android:text="5"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toEndOf="@+id/noFour"
            app:layout_constraintTop_toTopOf="@+id/noFour"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noSix"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="0dp"
            android:text="6"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toEndOf="@+id/noFive"
            app:layout_constraintTop_toTopOf="@+id/noFive"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noFour"
            android:layout_width="0dp"
            android:layout_height="0dp"

            android:text="4"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toStartOf="@+id/noSeven"
            app:layout_constraintTop_toBottomOf="@+id/noSeven"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noThree"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="3"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toEndOf="@+id/noTwo"
            app:layout_constraintTop_toTopOf="@+id/noTwo"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noZero"
            android:layout_width="0dp"
            android:layout_height="0dp"

            android:text="0"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toStartOf="@+id/noOne"
            app:layout_constraintTop_toBottomOf="@+id/noOne"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noTwo"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="2"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toEndOf="@+id/noOne"
            app:layout_constraintTop_toTopOf="@+id/noOne"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/noOne"
            android:layout_width="0dp"
            android:layout_height="0dp"

            android:text="1"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toStartOf="@+id/noFour"
            app:layout_constraintTop_toBottomOf="@+id/noFour"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/plusButton"
            android:layout_width="0dp"
            android:layout_height="0dp"

            android:text="+"
            android:textSize="50sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toStartOf="@+id/customPriceButton"
            app:layout_constraintTop_toBottomOf="@+id/customPriceButton"
            app:layout_constraintWidth_percent="0.25" />

        <Button

            android:id="@+id/halfPriceButton"
            android:layout_width="0dp"
            android:layout_height="0dp"

            android:text="50%"
            android:textSize="30sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toStartOf="@+id/buttonErase"
            app:layout_constraintTop_toBottomOf="@+id/buttonErase"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/customPriceButton"
            android:layout_width="0dp"
            android:layout_height="0dp"

            android:text="Trang trí"
            android:textSize="20sp"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintStart_toStartOf="@+id/halfPriceButton"
            app:layout_constraintTop_toBottomOf="@+id/halfPriceButton"
            app:layout_constraintWidth_percent="0.25" />

        <Button
            android:id="@+id/minusButton"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="Sua"
            android:textSize="30sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.25"
            app:layout_constraintTop_toTopOf="@+id/noZero"
            app:layout_constraintWidth_percent="0.25" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />


</androidx.constraintlayout.widget.ConstraintLayout>